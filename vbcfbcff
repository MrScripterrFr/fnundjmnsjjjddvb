coroutine.wrap(function()

while true do -- anim changer

if HumanDied then break end

sine = sine + speed

local rlegray = Ray.new(reanim["Right Leg"].Position + Vector3.new(0, 0.5, 0), Vector3.new(0, -2, 0))

